{
  "name": "nuxsaas",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config ./server/database/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./server/database/drizzle.config.ts",
    "build": "nuxt build",
    "serve": "node .output/server/index.mjs",
    "deploy": "wrangler deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.830.0",
    "@better-auth/stripe": "^1.2.12",
    "@nuxt/ui": "^3.2.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/seo": "^3.1.0",
    "@pinia/nuxt": "^0.11.1",
    "@vueuse/core": "13.4.0",
    "@vueuse/integrations": "^13.4.0",
    "better-auth": "^1.2.12",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "0.44.2",
    "drizzle-zod": "^0.8.2",
    "ioredis": "^5.6.1",
    "nuxt": "^4.0.0",
    "nuxt-charts": "^0.1.11",
    "ofetch": "^1.4.1",
    "pg": "^8.16.3",
    "pinia": "^3.0.3",
    "resend": "^4.6.0",
    "sortablejs": "^1.15.6",
    "stripe": "^18.3.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-component-type-helpers": "3.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@better-auth/cli": "^1.2.12",
    "@iconify-json/lucide": "^1.2.54",
    "@iconify-json/simple-icons": "^1.2.41",
    "@nuxt/eslint": "^1.5.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/pg": "^8.15.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "playwright-core": "^1.53.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.22.0"
  }
}
