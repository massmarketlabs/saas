{
  "name": "nuxsaas",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.20.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/internal/auth/schema.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config ./server/database/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./server/database/drizzle.config.ts",
    "build": "nuxt build",
    "serve": "node .output/server/index.mjs",
    "deploy": "wrangler deploy .output/server/index.mjs --assets .output/public   ",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/s3-request-presigner": "^3.930.0",
    "@internationalized/date": "^3.10.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.1.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "^10.2.0",
    "@nuxtjs/seo": "^3.2.2",
    "@pinia/nuxt": "^0.11.3",
    "@types/lodash.chunk": "^4.2.9",
    "@vueuse/core": "13.4.0",
    "@vueuse/integrations": "^13.4.0",
    "better-auth": "^1.3.34",
    "d3-dsv": "^3.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "0.44.7",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.8.3",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "nuxt": "^4.2.1",
    "nuxt-charts": "^1.0.2",
    "ofetch": "^1.5.1",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "resend": "^6.4.2",
    "sortablejs": "^1.15.6",
    "uuid": "^13.0.0",
    "vue": "^3.5.24",
    "vue-component-type-helpers": "3.1.3",
    "vue-countup-v3": "^1.4.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@better-auth/cli": "^1.3.34",
    "@iconify-json/lucide": "^1.2.73",
    "@iconify-json/simple-icons": "^1.2.58",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@types/pg": "^8.15.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "playwright-core": "^1.56.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
